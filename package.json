{
  "name": "mmm-cta",
  "version": "1.1.0",
  "description": "Magic Mirror Module for displaying CTA Train and Bus arrival times",
  "main": "MMM-CTA.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "lint": "eslint .",
    "fix": "eslint . --fix"
  },
  "author": "Jordan Welch",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "eslint": "^8.43.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "fetch-mock": "^11.1.5",
    "jest": "^29.5.0",
    "nunjucks": "^3.2.4"
  },
  "keywords": [
    "magicmirror",
    "cta",
    "train",
    "bus",
    "tracker"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JHWelch/MMM-CTA.git"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [],
    "parserOptions": {
      "ecmaVersion": "latest"
    },
    "rules": {
      "no-undef": "off",
      "no-underscore-dangle": "off",
      "import/no-unresolved": "off"
    }
